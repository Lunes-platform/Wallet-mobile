{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test:unit": "jest --forceExit",
    "test": "npm run eslint && npm run flow && jest --forceExit",
    "test:watch": "jest --watch",
    "flow": "flow",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage",
    "eslint": "eslint ./src",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "precommit": "lint-staged && npm test",
    "postinstall": "react-native link",
    "clear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clear:android": "cd android/ && ./gradlew clean",
    "clear:cache": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && gradlew assembleRelease && gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "apisauce": "^0.14.3",
    "big.js": "^5.0.3",
    "lunes-lib": "0.0.26",
    "native-base": "^2.4.2",
    "ramda": "^0.25.0",
    "react": "16.3.0-alpha.1",
    "react-native": "0.54.0",
    "react-native-behavior": "0.0.18",
    "react-native-camera": "^1.0.3",
    "react-native-firebase": "^4.0.3",
    "react-native-fs": "^2.9.12",
    "react-native-i18n": "^2.0.12",
    "react-native-image-resizer": "^1.0.0",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-photo-upload": "^1.2.0",
    "react-native-qrcode": "^0.2.6",
    "react-native-qrcode-scanner": "^1.0.1",
    "react-native-svg": "^6.3.1",
    "react-native-svg-charts": "^4.2.0",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^1.5.8",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-persist": "^4.10.2",
    "redux-saga": "^0.16.0",
    "reduxsauce": "^0.7.0",
    "seamless-immutable": "^7.1.3"
  },
  "devDependencies": {
    "babel-preset-react-native-stage-0": "^1.0.1",
    "jest": "^22.4.3",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "16.3.0-alpha.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-preset-flow": "^6.23.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.68.0",
    "flow-typed": "^2.4.0",
    "husky": "^0.14.3",
    "ignite-i18n": "^1.1.1",
    "jest-cli": "^22.4.3",
    "jest-enzyme": "^6.0.0",
    "jest-serializer-enzyme": "^1.0.0",
    "jsdom": "11.7.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "^7.0.1",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "react-native-mock": "^0.3.1",
    "reactotron-react-native": "^1.14.0",
    "reactotron-redux": "^1.13.0",
    "reactotron-redux-saga": "^1.13.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "remote-redux-devtools": "^0.5.12",
    "remotedev-rn-debugger": "^0.8.3"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "./src/**/**/*.js"
    ],
    "testMatch": [
      "*/src/tests/**/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./src/tests/setup"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native|react-native-firebase|react-native-google-signin|react-native-fbsdk|native-base|native-base-shoutem-theme|@shoutem/theme|@shoutem/animation|@shoutem/ui|tcomb-form-native|react-navigation|lunes-lib)"
    ],
    "setupFiles": [
      "./src/tests/setup"
    ]
  }
}
